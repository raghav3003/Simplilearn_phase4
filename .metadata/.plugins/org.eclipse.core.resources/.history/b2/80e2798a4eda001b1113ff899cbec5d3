const age = 20;
let percentage =61;

const promise1 = new Promise(function(resolve, reject) {
	if (age > 18 && age < 30) {
		resolve("Success");
	} else {
		reject("Invalid age");
	}
})

promise1

	.then(function success(result) {
		console.log("success");
	})
	
	.catch(function failure(result) {
		console.log("reject");
	})
	
	.finally(function final() {
		console.log("we are into final block");
	})
	
