<body>
<script>
	
	function Employee (firstName, lastName) {
		this.firstName = firstName;
		this.lastName = lastName;
	}
	
	Employee.prototype.fullName = function () {
		return this.firstName + ' ' + this.lastName;
	}
	
	var emp1 = new Employee('Charles', 'Darwin');
	var emp2 = new Employee('Charles', 'Leclerc');
	
	document.write(emp1.fullName() + '<br>');
	document.write(emp2.fullName() + '<br>');
	
	Employee.prototype.company = 'AMG';
	
	var emp1 = new Employee('Charles', 'Darwin');
	var emp2 = new Employee('Charles', 'Leclerc');
	
	document.write(emp1.firstName + ' ' + emp1.lastName + ' ' + emp1.company + '<br>');
	document.write(emp2.firstName + ' ' + emp2.lastName + ' ' + emp2.company + '<br>');
	document.write(emp1.fullName() + ' ' + emp2.fullName());
	
	function Stud () {
		this.name = 'John';
		this.gender = 'M';
	}
	
	var studObj = new Stud();
	
	Stud.prototype.sayHi = function () {
		alert('Hi');
	}
	
	
	
	
	
</script>
</body>
</html>