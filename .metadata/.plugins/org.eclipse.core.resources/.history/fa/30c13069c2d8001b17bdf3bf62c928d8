<body>
<script>
	
	function Employee (firstName, lastName) {
		this.firstName = firstName;
		this.lastName = lastName;
	}
	
	Employee.prototype.fullName = function () {
		return this.firstName + ' ' + this.lastName;
	}
	
	var emp1 = new Employee('Charles', 'Darwin');
	var emp2 = new Employee('Charles', 'Leclerc');
	
	document.write(emp1.fullName() + '<br>');
	document.write(emp2.fullName() + '<br>');
	
	Employee.prototype.company = 'AMG';
	
	var emp1 = new Employee('Charles', 'Darwin');
	var emp2 = new Employee('Charles', 'Leclerc');
	
	document.write(emp1.firstName + ' ' + emp1.lastName + ' ' + emp1.company + '<br>');
	document.write(emp2.firstName + ' ' + emp2.lastName + ' ' + emp2.company + '<br>');
	document.write(emp1.fullName() + ' ' + emp2.fullName());
	
	function Student () {
		this.name = 'Charled';
		this.gender = 'Male';
	}
	
	Student.prototype.age = 15;
	
	var studObj1 = new Student();
	alert(studObj1.age);
	
	var studObj2 = new Student();
	alert(studObj2.age);
	
	Student.prototype.age = 35;
	
	var studObj3 = new Student();
	alert(studObj3.age);
	
	var studObj = new Student();
	document.write(Student.prototype + '<br>');
	document.write(studObj.prototype + '<br>');
	document.write(studObj._proto_ + '<br>');
	
	document.write(typeof Student.prototype + '<br>');
	document.write(typeof studObj.prototype + '<br>');
	document.write(Student.prototype === studObj._proto_ + '<br>');
	
	
	function Stud () {
		this.name = 'John';
		this.gender = 'M';
	}
	
	var studObj = new Stud();
	
	Stud.prototype.sayHi = function () {
		alert('Hi');
	}
	
	
	
	
	
</script>
</body>
