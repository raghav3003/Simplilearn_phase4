<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>

	<script type="text/javascript">
	let cat = {type: 'Cat', sound: 'Meaw'};
	let dog = {type: 'Dog', cound: 'Woof'};
	let say = function(greeting){
		document.write(greeting);
		document.write("<hr>");
		document.write(this.type+' says '+this.sound);
		document.write("<hr><hr>");
	};
	say.apply(cat, ['Hi']);
	say.apply(dog,['Hi']);
	say.call(cat, 'Hoo');
	say.call(dog,'Hoo');
	let car = {
			speed:5,
			start: function(){
				document.write('Starts with '+this.speed+" km/h");
			}
	};
	let aircraft ={
			speed:10,
			fly: function(){
				document.write('Flying');
			}
	};
	let taxiing = car.start.bind(aircraft);
	taxiing();
		
	</script>

</body>
</html>